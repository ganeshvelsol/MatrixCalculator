<LinearLayout android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="3">
        <EditText
            android:id="@+id/third_et6"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:hint="one"
            android:inputType="numberDecimal"

            android:layout_margin="2dp"
            android:ems="3"/>
        <EditText
            android:id="@+id/third_et7"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="numberDecimal"
            android:layout_weight="1"
            android:layout_margin="2dp"
            android:hint="two"
            android:ems="3"/>
        <EditText
            android:id="@+id/third_et8"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="numberDecimal"
            android:layout_weight="1"
            android:layout_margin="2dp"
            android:hint="three"
            android:ems="3"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:weightSum="3">
        <EditText
            android:id="@+id/fourth_et9"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:hint="four"
            android:layout_margin="2dp"
            android:inputType="numberDecimal"
            android:ems="3"/>
        <EditText
            android:id="@+id/fifth_et10"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:inputType="numberDecimal"
            android:layout_weight="1"
            android:layout_margin="2dp"
            android:hint="five"
            android:ems="3"/>
        <EditText
            android:id="@+id/fifth_et11"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:inputType="numberDecimal"
            android:layout_weight="1"
            android:layout_margin="2dp"
            android:hint="six"
            android:ems="3"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:weightSum="3">
        <EditText
            android:id="@+id/fourth_et12"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:hint="seven"
            android:layout_margin="2dp"
            android:inputType="numberDecimal"
            android:ems="3"/>
        <EditText
            android:id="@+id/fifth_et13"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:inputType="numberDecimal"
            android:layout_weight="1"
            android:layout_margin="2dp"
            android:hint="eight"
            android:ems="3"/>
        <EditText
            android:id="@+id/fifth_et14"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:inputType="numberDecimal"
            android:layout_weight="1"
            android:layout_margin="2dp"
            android:hint="nine"
            android:ems="3"/>
    </LinearLayout>
</LinearLayout>