<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="calci.matrix.matrixcalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".addition.AdditionExample" />
        <activity android:name=".addition.Addition"
            android:label="Addition"/>
        <activity android:name=".subtraction.SubtractionExample" />
        <activity android:name=".subtraction.Subtraction"
            android:label="Subtraction"/>
        <activity android:name=".DiagonalMatrix"
            android:label="Diagonal Matrix"/>
        <activity android:name=".determenent.DeterminentMatrix"
            android:label="Determinent"/>
        <activity android:name=".inverse.InverseMatrix">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".multipication.Multipication" />
        <activity android:name=".UpperTrianguar"
            android:label="Upper Triangular"/>
        <activity android:name=".lowertriangular.LowerTriangular"
            android:label="Lower Tringular">
        </activity>
    </application>

</manifest>